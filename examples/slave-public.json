{
    "id": "nginx",
    "container": {
    "type": "DOCKER",
    "docker": {
          "image": "mesosphere/simple-docker",
          "network": "BRIDGE",
          "portMappings": [
            { "hostPort": 8088, "containerPort": 80, "protocol": "tcp"}
          ]
        }
    },
    "acceptedResourceRoles": ["slave_public"],
    "instances": 1,
    "cpus": 0.1,
    "mem": 64
}
