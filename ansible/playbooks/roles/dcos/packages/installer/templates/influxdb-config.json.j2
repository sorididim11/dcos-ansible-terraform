{
  "service": {
    "name": "{{service.name}}"
  },
  "storage": {
    "pre_create_database": true,
    "pre_create_database_name": "cadvisor",
    "host_volume_influxdb": "{{ service.options.host_db_volume_path | default('/tmp') }}"
  },
  "networking": {
    "port": 8083,
    "port_api": 8086,
    "external_access": {
      "enable": false,
      "external_access_port": 18083
    }
  }
}